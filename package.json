{
  "name": "ai-workflows",
  "private": true,
  "packageManager": "pnpm@9",
  "workspaces": [
    "packages/*",
    "examples/*",
    "templates/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "biome check .",
    "format": "biome format . --write",
    "release": "changeset version && pnpm -r build && changeset publish",
    "test": "turbo test",
    "clean": "turbo clean && rimraf node_modules"
  }
}